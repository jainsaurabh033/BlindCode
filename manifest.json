{
  "manifest_version": 3,
  "name": "LeetCode Difficulty Hider",
  "version": "1.0",
  "description": "Hides Easy, Medium, and Hard difficulty labels on leetcode",
  "permissions": ["scripting"],
  "content_scripts": [
    {
      "matches": ["*://leetcode.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "icon.png"
  }
}